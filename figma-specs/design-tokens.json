{
  "name": "TrafficNavigator",
  "version": "1.0",
  "description": "Figma Design Tokens Export",
  "tokens": {
    "colors": {
      "primary": {
        "default": "#6C63FF",
        "light": "#8B5CF6",
        "dark": "#5B52E0"
      },
      "route": {
        "fast": "#10B981",
        "fastLight": "#D1FAE5",
        "fastDark": "#059669",
        "moderate": "#F59E0B",
        "moderateLight": "#FEF3C7",
        "moderateDark": "#D97706",
        "heavy": "#EF4444",
        "heavyLight": "#FEE2E2",
        "heavyDark": "#DC2626"
      },
      "neutral": {
        "white": "#FFFFFF",
        "background": "#F8FAFC",
        "card": "#FFFFFF",
        "border": "#E2E8F0",
        "textPrimary": "#0F172A",
        "textSecondary": "#64748B",
        "textTertiary": "#94A3B8",
        "dark": "#1E293B"
      }
    },
    "gradients": {
      "primary": {
        "type": "linear",
        "angle": 135,
        "stops": [
          { "color": "#6C63FF", "position": 0 },
          { "color": "#8B5CF6", "position": 100 }
        ]
      },
      "splash": {
        "type": "linear",
        "angle": 135,
        "stops": [
          { "color": "#0f0c29", "position": 0 },
          { "color": "#302b63", "position": 50 },
          { "color": "#24243e", "position": 100 }
        ]
      }
    },
    "typography": {
      "fontFamily": {
        "primary": "SF Pro Display",
        "secondary": "SF Pro Text",
        "mono": "SF Mono"
      },
      "styles": {
        "h1": { "size": 32, "weight": 700, "lineHeight": 40 },
        "h2": { "size": 24, "weight": 600, "lineHeight": 32 },
        "h3": { "size": 20, "weight": 600, "lineHeight": 28 },
        "bodyLarge": { "size": 16, "weight": 400, "lineHeight": 24 },
        "body": { "size": 14, "weight": 400, "lineHeight": 20 },
        "bodySmall": { "size": 12, "weight": 400, "lineHeight": 16 },
        "caption": { "size": 10, "weight": 500, "lineHeight": 14 },
        "button": { "size": 16, "weight": 600, "lineHeight": 24 }
      }
    },
    "spacing": {
      "xs": 4,
      "sm": 8,
      "md": 12,
      "base": 16,
      "lg": 20,
      "xl": 24,
      "2xl": 32,
      "3xl": 40,
      "4xl": 48,
      "5xl": 64
    },
    "borderRadius": {
      "sm": 8,
      "md": 12,
      "lg": 16,
      "xl": 20,
      "2xl": 24,
      "full": 9999
    },
    "shadows": {
      "sm": "0 1px 2px rgba(0,0,0,0.05)",
      "md": "0 4px 6px rgba(0,0,0,0.07)",
      "lg": "0 10px 25px rgba(0,0,0,0.1)",
      "xl": "0 20px 40px rgba(0,0,0,0.15)"
    },
    "screens": {
      "iPhone15Pro": {
        "width": 393,
        "height": 852,
        "safeAreaTop": 59,
        "safeAreaBottom": 34,
        "cornerRadius": 55,
        "screenRadius": 47
      }
    }
  },
  "components": {
    "button": {
      "primary": {
        "height": 56,
        "borderRadius": 16,
        "background": "gradient.primary",
        "textColor": "#FFFFFF",
        "fontSize": 16,
        "fontWeight": 600,
        "shadow": "0 4px 15px rgba(108,99,255,0.4)"
      },
      "secondary": {
        "height": 56,
        "borderRadius": 16,
        "background": "#F8FAFC",
        "borderColor": "#E2E8F0",
        "borderWidth": 1,
        "textColor": "#0F172A",
        "fontSize": 16,
        "fontWeight": 600
      },
      "icon": {
        "size": 44,
        "borderRadius": 12,
        "background": "#F8FAFC"
      }
    },
    "input": {
      "height": 56,
      "borderRadius": 16,
      "background": "#F8FAFC",
      "borderColor": "#E2E8F0",
      "borderWidth": 2,
      "paddingX": 50,
      "fontSize": 15,
      "iconSize": 16,
      "focusBorderColor": "#6C63FF"
    },
    "searchBar": {
      "height": 56,
      "borderRadius": 20,
      "background": "#FFFFFF",
      "shadow": "lg",
      "iconColor": "#94A3B8",
      "micButtonSize": 42
    },
    "routeCard": {
      "padding": 16,
      "borderRadius": 16,
      "colorBarWidth": 4,
      "colorBarHeight": 60,
      "goButtonSize": 48
    },
    "bottomNav": {
      "height": 85,
      "paddingBottom": 25,
      "iconSize": 22,
      "labelSize": 10,
      "activeColor": "#6C63FF",
      "inactiveColor": "#94A3B8"
    }
